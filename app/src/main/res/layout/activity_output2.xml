<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OutputActivity2">

    <ImageView
        android:id="@+id/IMG2"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="190dp"
        android:src="@drawable/file"
        />
    <TextView
        android:layout_below="@id/IMG2"
        android:id="@+id/tv_labelnama"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        android:textSize="25dp"
        android:text="Nama :"/>

    <TextView
        android:layout_below="@id/tv_labelnama"
        android:id="@+id/tv_labelnim"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:textSize="25dp"
        android:text="NIM    :"/>
    <TextView
        android:layout_below="@id/tv_labelnim"
        android:id="@+id/tv_labelnilai"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:textSize="25dp"
        android:text="Nilai    :"/>
    <TextView
        android:layout_below="@id/tv_labelnim"
        android:layout_toRightOf="@id/tv_labelnilai"
        android:id="@+id/tv_nilai"
        android:layout_width="260dp"
        android:layout_height="35dp"
        android:layout_marginTop="15dp"
        android:textSize="25dp" />
    <TextView
        android:layout_below="@id/tv_labelnama"
        android:layout_toRightOf="@id/tv_labelnim"
        android:id="@+id/tv_nim"
        android:layout_width="260dp"
        android:layout_height="35dp"
        android:layout_marginTop="15dp"
        android:textSize="25dp" />
    <TextView
        android:layout_below="@id/IMG2"
        android:layout_toRightOf="@id/tv_labelnama"
        android:id="@+id/tv_nama"
        android:layout_width="260dp"
        android:layout_height="35dp"
        android:layout_marginTop="50dp"
        android:textSize="25dp" />


</RelativeLayout>